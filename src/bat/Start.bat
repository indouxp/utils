@echo off

:::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::
: StartCRS.bat実行
:::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::
call %~dp0StartCRS.bat
set RC=%errorlevel%
:if %RC% neq 0 goto :err_010
if %RC% neq 0 echo StartCRS ERR

:::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::
: StartInstance.bat実行
:::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::
call %~dp0StartInstance.bat %COMPUTERNAME% orcl
set RC=%errorlevel%
if %RC% neq 0 goto :err_020

:::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::
:done
:::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::
  echo インスタンス起動成功
  exit /b 0

:::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::
:err_010
:::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::
  echo StartCRS.bat失敗
  exit /b %RC%

:::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::
:err_020
:::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::
  echo StartInstance.bat失敗
  exit /b %RC%
